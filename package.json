{
	"name": "novel-studio",
	"version": "1.0.0",
	"main": "expo-router/entry",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"gen": "supabase gen types typescript --project-id pxshvmbgzfneqkcaiyky --schema public > src/utils/database.types.ts",
		"build:dev": "eas build --profile development --platform ios",
		"build": "cmd /C eas build --profile production --platform ios --auto-submit && yarn commit",
		"build:pv": "eas build --profile preview --platform ios",
		"submit": "eas submit --platform ios",
		"update": "eas update --platform ios",
		"commit": "cmd /V /C \"for /f %i in ('jq -r \".expo.ios.buildNumber\" app.json') do set version=%i && git add . && git commit -m \"ver. !version!\""
	},
	"dependencies": {
		"@e-state/react": "^0.1.16",
		"@expo-google-fonts/gothic-a1": "^0.2.3",
		"@expo-google-fonts/noto-sans-jp": "^0.2.3",
		"@expo-google-fonts/noto-serif-jp": "^0.2.3",
		"@expo-google-fonts/roboto": "^0.2.3",
		"@expo/webpack-config": "^19.0.0",
		"@react-native-async-storage/async-storage": "1.18.2",
		"@react-navigation/drawer": "^6.6.6",
		"@supabase/supabase-js": "^2.38.5",
		"expo": "~49.0.15",
		"expo-auth-session": "~5.0.2",
		"expo-blur": "~12.4.1",
		"expo-constants": "~14.4.2",
		"expo-dev-client": "~2.4.12",
		"expo-linking": "~5.0.2",
		"expo-localization": "~14.3.0",
		"expo-router": "^2.0.0",
		"expo-status-bar": "~1.6.0",
		"expo-updates": "~0.18.17",
		"expo-web-browser": "~12.3.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "0.72.6",
		"react-native-context-menu-view": "^1.14.1",
		"react-native-draggable-flatlist": "^4.0.1",
		"react-native-elements": "^3.4.3",
		"react-native-gesture-handler": "~2.12.0",
		"react-native-reanimated": "~3.3.0",
		"react-native-safe-area-context": "4.6.3",
		"react-native-screens": "~3.22.0",
		"react-native-url-polyfill": "^2.0.0",
		"react-native-web": "~0.19.6"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@types/react": "~18.2.14",
		"typescript": "^5.3.3"
	},
	"private": true,
	"volta": {
		"node": "16.20.2",
		"yarn": "3.7.0"
	}
}
