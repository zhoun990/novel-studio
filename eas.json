{
  "cli": {
    "version": ">= 5.9.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_ENV": "development"
			},
			"channel": "staging"
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"APP_ENV": "preview"
			},
			"channel": "staging"
		},
		"production": {
			"env": {
				"APP_ENV": "production"
			},
			"autoIncrement": true,
			"channel": "production"
		}
  },
  "submit": {
    "production": {}
  }
}
